@model CodeTheWay.Web.Ui.Models.MyViewModels.WeatherViewModel.WeatherViewModel
@*
    ViewData["Title"] = "Create"
*@

<h1>Create</h1>
<form asp-action="Register">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Date" class="control-label"></label>
        <input asp-for="Date" class="form-control" />
        <span asp-validation-for="Date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="HighTemp" class="control-label"></label>
        <input asp-for="HighTemp" class="form-control" />
        <span asp-validation-for="HighTemp" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LowTemp" class="control-label"></label>
        <input asp-for="LowTemp" class="form-control" />
        <span asp-validation-for="LowTemp" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="AvgWindSpeed" class="control-label"></label>
        <input asp-for="AvgWindSpeed" class="form-control" />
        <span asp-validation-for="AvgWindSpeed" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="TotalPrecipitation" class="control-label"></label>
        <input asp-for="TotalPrecipitation" class="form-control" />
        <span asp-validation-for="TotalPrecipitation" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>